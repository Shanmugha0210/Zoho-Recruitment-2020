#include <stdio.h>

int main()
{
   char a[100];int i=0;int j=0;
   int k;
   char b[100];
   scanf("%s",a);
   while(a[i]!=NULL)
   {
       i++;
   }
   if(i%2!=0)
   {
        for(k=i/2;k<i&&k>j;k++)
        {
            b[j++]=a[k];
            printf("%s\n",b);
        }
        for(k=0;k<i/2;k++)
        {
            b[j++]=a[k];
            printf("%s\n",b);
        }
   }
   else
   {
       printf("Only Word with Odd Letter Count are Allowed");
   }
    return 0;
}
